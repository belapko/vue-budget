    "eslint": "^8.49.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vue": "^9.17.0",
    "fibers": "^5.0.3",
    "jsdom": "^22.1.0",
    "json-server": "^0.17.4",
    "json-server-auth": "^2.1.0",
    "nodemon": "^3.0.1",
    "npm-run-all2": "^6.0.6",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.2",
    "storybook": "^7.4.6",
    "storybook-addon-themes": "^6.1.0",
